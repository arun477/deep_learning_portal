   "source": [
    "# n: nmlogm\n",
    "# s: n\n",
    "\n",
    "from collections import defaultdict\n",
    "\n",
    "class Solution:\n",
    "    def groupAnagrams(self, strs):\n",
    "        hmap = defaultdict(list)\n",
    "        for s in strs:\n",
    "            hmap[tuple(sorted(s))].append(s)\n",
    "        return list(hmap.values())"
   ]
